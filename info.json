---
layout: none
---
{%- assign osm-sandbox = site.data.index.entries.osm-sandbox | sort: 'created' | reverse -%}

{%- assign osm_sandbox_latest = osm-sandbox | first -%}

{%- capture "osm_sandbox_stable_version" -%}
{%- for chart in osm-sandbox -%}
    {%- unless chart.version contains "-" -%}
        {{ chart.version }};
    {%- endunless -%}
{%- endfor -%}
{%- endcapture -%}
{%- assign osm_sandbox_stable_version = osm_sandbox_stable_version | split: ";" | first -%}

{%- capture "osm_sandbox_pre_version" -%}
{%- for chart in osm-sandbox -%}
    {%- assign dots_count_after_dash = chart.version | split: "-" | shift | first | split: "." | size | minus: 1 -%}
    {%- assign first_char_after_dash = chart.version | split: "-" | shift | first | slice: 0 -%}
    {%- if chart.version contains "-" -%}
        {%- if dots_count_after_dash == 1 -%}
            {%- if first_char_after_dash != "n" -%}
                {{ chart.version }};
            {%- endif -%}
        {%- endif -%}
    {%- else -%}
        {{ chart.version }};
    {%- endif -%}
{%- endfor -%}
{%- endcapture -%}
{%- assign osm_sandbox_pre_version = osm_sandbox_pre_version | split: ";" | first -%}


{
    "osm-sandbox":
        {
            "stable": "{{ osm_sandbox_stable_version }}",
            "pre": "{{ osm_sandbox_pre_version }}",
            "dev": "{{ osm_sandbox_latest.version }}",
            "latest": "{{ osm_sandbox_latest.version }}"
        }
}
